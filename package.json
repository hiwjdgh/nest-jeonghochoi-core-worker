        "axios": "^1.13.2",
        "axios-retry": "^4.5.0",
        "basic-ftp": "^5.1.0",
        "csv-stringify": "^6.6.0",
        "exceljs": "^4.4.0",
        "handlebars": "^4.7.8",
        "mssql": "^12.2.0",
        "mysql2": "^3.16.3",
        "nodemailer": "^8.0.1",
        "pg": "^8.18.0",
        "pino": "^10.2.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "zod": "^4.3.6"
