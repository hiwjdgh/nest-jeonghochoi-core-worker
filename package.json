{
    "name": "@jeonghochoi/core-worker",
    "version": "0.0.7",
    "description": "NestJS worker infra modules",
    "license": "MIT",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
